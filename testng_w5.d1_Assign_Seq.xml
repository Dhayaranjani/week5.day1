      <class name="week5.day1.CreateLead"/>
      <class name="week5.day1.DeleteLead"/>
      <class name="week5.day1.DuplicateLead"/>
      <class name="week5.day1.EditLead"/>
      <class name="week5.day1.Login"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
